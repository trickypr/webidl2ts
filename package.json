{
  "name": "webidl2ts",
  "version": "2.0.0",
  "description": "Converts Web IDL to Typescript (.d.ts)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && chmod 755 dist/cli.js",
    "lint": "tsc --noEmit",
    "start": "pnpm build && ./dist/cli.js",
    "format": "prettier --write .",
    "test": "ava ./test/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giniedp/webidl2ts.git"
  },
  "homepage": "https://github.com/giniedp/webidl2ts",
  "author": "Alexander Gr√§fenstein",
  "license": "MIT",
  "files": [
    "dist",
    "Readme.md",
    "package.json",
    "LICENSE.md"
  ],
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/webidl2": "^23.13.5",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "ava": "^5.1.1",
    "eslint": "^7.12.1",
    "prettier": "^2.1.2",
    "ts-node": "^10.0.0"
  },
  "dependencies": {
    "jsdom": "^21.0.0",
    "typescript": "^4.9.3",
    "webidl2": "^22.2.0",
    "yargs": "^17.6.2"
  },
  "bin": {
    "webidl2ts": "./dist/cli.js"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
